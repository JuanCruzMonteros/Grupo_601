<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:ems="10"
        android:enabled="false"
        android:inputType="textPersonName"
        android:text="Lista Historica de migrañas"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="59dp" />

    <TextView
        android:id="@+id/txtHistorias"
        android:layout_width="377dp"
        android:layout_height="287dp"
        android:layout_marginTop="20dp"
        android:inputType="textMultiLine"
        android:text="No hay eventos de migrañas ingresados actualmente."
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        tools:layout_editor_absoluteX="18dp" />

    <Button
        android:id="@+id/btn_EliminarLista"
        android:layout_width="360dp"
        android:layout_height="39dp"
        android:layout_marginTop="36dp"
        android:text="Eliminar Lista"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtHistorias" />

</androidx.constraintlayout.widget.ConstraintLayout>